{
    "name":"books-server",
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel /node"
        },
        {
            "src": "/livros.json",
            "use":"@vercel /node"
        },
        {
            "src": "/favoritos.json",
            "use":"@vercel /node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/livros.json",
            "dest": "livros.json"
        },
        {
            "src": "/favoritos.json",
            "dest": "favoritos.json"
        }
    ]
}